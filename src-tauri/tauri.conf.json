{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "Guess App",
  "version": "0.1.0",
  "identifier": "com.dailyhills.guessinggame",
  "build": {
    "frontendDist": "../dist",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "app": {
    "windows": [
      {
        "title": "Guess",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon_16x16.png",
      "icons/icon_16x16x2.png",
      "icons/icon_32x32.png",
      "icons/icon_32x32x2.png",
      "icons/icon_128x128.png",
      "icons/icon_128x128x2.png",
      "icons/icon_256x256.png",
      "icons/icon_256x256x2.png",
      "icons/icon_512x512.png",
      "icons/icon_512x512x2.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }  
}
